{
  "name": "starklens",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "chmod +x kysely_generate.sh",
    "dev": "chmod +x kysely_generate.sh && ./kysely_generate.sh && next dev",
    "build": "chmod +x kysely_generate.sh && ./kysely_generate.sh -y && next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "./kysely_generate.sh",
    "migrate:up": "TS_NODE_PROJECT=tsconfig.migrations.json ts-node library/db/migrations/index.ts up",
    "migrate:down": "TS_NODE_PROJECT=tsconfig.migrations.json ts-node library/db/migrations/index.ts down"
  },
  "dependencies": {
    "@dynamic-labs/ethereum": "^2.2.0",
    "@dynamic-labs/iconic": "^2.2.1",
    "@dynamic-labs/sdk-react-core": "^2.2.0",
    "@dynamic-labs/starknet": "2.2.0",
    "@dynamic-labs/wagmi-connector": "^2.2.0",
    "@tanstack/react-query": "^5.45.1",
    "@vercel/postgres-kysely": "^0.8.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.10",
    "kysely": "^0.27.3",
    "kysely-codegen": "^0.15.0",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.3.0",
    "viem": "2.x",
    "wagmi": "^2.10.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/yargs": "^17.0.32",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
